{
  "expo": {
    "name": "Dots & Boxes",
    "slug": "dots-and-boxes-game",
    "scheme": "dotsandboxes",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.louislam09.dotsandboxes"
    },
    "android": {
      "icon": "./src/assets/images/icon.png",
      "adaptiveIcon": {
        "backgroundColor": "#6366F1"
      },
      "package": "com.louislam09.dotsandboxes"
    },
    "web": {
      "output": "static",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-audio",
        {
          "microphonePermission": false
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos to set your avatar."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
